    "scroll-step": 2,
    "smooth-scrolling-threshold": 1,
    "on-click": "omarchy-launch-audio",
    "on-click-right": "pamixer -t"
  },
  "mpris": {
    "format": "Û∞ãé  {artist} - {title}",
    "format-paused": "<i><span color='grey'>{status_icon} {artist}</span></i>",
    "tooltip-format": "{album}",
    "max-length": 35,
