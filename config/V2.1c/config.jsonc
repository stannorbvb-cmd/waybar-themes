    "hyprland/workspaces",
    "idle_inhibitor",
    "custom/updatespacman",
    "group/tray-expander",
    "mpris"
  ],
  "modules-center": [
    "clock",
    "custom/voxtype",
    "custom/screenrecording-indicator",
    "custom/update",
    "custom/weather"
  ],
  "modules-right": [
    "network",
    "pulseaudio#output",
    "pulseaudio#input",
    "power-profiles-daemon",
    "memory",
    "cpu",
    "battery"
  ],
  "hyprland/workspaces": {
