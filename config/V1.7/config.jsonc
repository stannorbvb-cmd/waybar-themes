{
  "exclusive": true,
  "reload_style_on_change": true,
  "position": "top",
  "spacing": 0,
  "height": 0,
  "width": 1440,
  "margin-top": 0,
  "margin-bottom": 0,
  "modules-left": [
    "hyprland/workspaces",
    "custom/separator",
    "custom/windowpill"
  ],
  "modules-center": [
    "clock",
    "custom/idle-indicator",
    "custom/notification-silencing-indicator",
    "custom/voxtype",
    "custom/screenrecording-indicator",
    "custom/update",
    "custom/updatespacman"
  ],
  "modules-right": ["group/right2", "group/right"],

   "group/right2": {
    "orientation": "inherit",
    "modules": ["mpris"]
  },
 
  "group/right": {
    "orientation": "inherit",
    "modules": [
    "group/tray-expander",
    "network",
    "pulseaudio",
    "memory",
    "cpu",
    "battery"
  ]
  },

  "hyprland/workspaces": {
	"all-outputs": true,
	"format": "{icon}",
	"show-special": false,
	"on-click": "activate",
	"on-scroll-up": "hyprctl dispatch workspace e+1",
	"on-scroll-down": "hyprctl dispatch workspace e-1",
    "persistent-workspaces": {
      "*": 5
	},
	"format-icons": {
		"1": "I",
		"2": "II",
		"3": "III",
		"4": "IV",
		"5": "V",
		"6": "VI",
		"7": "VII",
		"8": "VIII",
		"9": "IX",
		"10": "X"
	}
},
"custom/separator": {
    "format": "|", 
    "tooltip": false
},
    "custom/windowpill": {
    "format": "{}",
    "exec": "~/.config/waybar/window_pill.py",
    "return-type": "json",
    "restart-interval": 1,
    "align": 0,
    "justify": "left"
  },
  "custom/update": {
    "format": "ÔÄ°",
    "exec": "omarchy-update-available",
    "on-click": "omarchy-launch-floating-terminal-with-presentation omarchy-update",
    "tooltip-format": "Omarchy update available",
    "signal": 7,
    "interval": 21600
  },
  "custom/updatespacman": {
    "format": "{}{icon}",
    "return-type": "json",
    "format-icons": {
      "has-updates": " Û∞èñ",
      "updated": ""
    },
    "exec-if": "which waybar-module-pacman-updates",
    "exec": "waybar-module-pacman-updates --no-zero-output"
  },
   "custom/voxtype": {
    "exec": "omarchy-voxtype-status",
    "return-type": "json",
    "format": "{icon}",
    "format-icons": {
      "idle": "",
      "recording": "Û∞ç¨",
      "transcribing": "Û∞îü"
    },
    "tooltip": true,
    "on-click-right": "omarchy-voxtype-config",
    "on-click": "omarchy-voxtype-model"
  },
    "custom/idle-indicator": {
    "on-click": "omarchy-toggle-idle",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/idle.sh",
    "signal": 9,
    "return-type": "json"
  },
  "custom/notification-silencing-indicator": {
    "on-click": "omarchy-toggle-notification-silencing",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/notification-silencing.sh",
    "signal": 10,
    "return-type": "json"
  },
  "cpu": {
    "interval": 2,
    "format": "<span size='6pt'>CPU </span>{usage}%",
    "on-click": "omarchy-launch-or-focus-tui btop"
  },
    "memory": {
    "interval": 2,
    "format": "<span size='6pt'>MEM </span>{percentage}%",
    "on-click": "omarchy-launch-or-focus-tui btop"
  },
  "clock": {
    "format": "{:%H:%M}",
    "format-alt": "{:L%d %B W%V %Y}",
    "tooltip-format": "<span>{calendar}</span>",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "on-click-right": "mode",
      "format": {
        "month": "<span color='#ffead3'><b>{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b>{}</b></span>"
      }
    },
    "on-click-right": "omarchy-launch-floating-terminal-with-presentation omarchy-tz-select"
  },
  "network": {
    "format-icons": [
      "Û∞§Ø",
      "Û∞§ü",
      "Û∞§¢",
      "Û∞§•",
      "Û∞§®"
    ],
    "format": "{icon}",
    "format-wifi": "<span size='6pt'>NET </span>{essid}",
    "format-ethernet": "<span size='6pt'>NET </span>{bandwidthDownBytes}",
    "format-disconnected": "Û∞§Æ",
    "tooltip-format-wifi": "{essid} ({frequency} GHz)\n‚á£{bandwidthDownBytes}  ‚á°{bandwidthUpBytes}",
    "tooltip-format-ethernet": "‚á£{bandwidthDownBytes}  ‚á°{bandwidthUpBytes}",
    "tooltip-format-disconnected": "Disconnected",
    "interval": 3,
    "spacing": 1,
    "on-click": "omarchy-launch-wifi"
  },
  "battery": {
    "format": "<span size='6pt'>BAT </span>{capacity}%",
    "format-discharging": "<span size='6pt'>BAT </span>{capacity}%",
    "format-charging": "<span size='6pt'>BAT_c </span>{capacity}%",
    "format-plugged": "<span size='6pt'>BAT_p </span>{capacity}%",
    "format-icons": {
      "charging": [
        "Û∞¢ú",
        "Û∞ÇÜ",
        "Û∞Çá",
        "Û∞Çà",
        "Û∞¢ù",
        "Û∞Çâ",
        "Û∞¢û",
        "Û∞Çä",
        "Û∞Çã",
        "Û∞ÇÖ"
      ],
      "default": [
        "Û∞Å∫",
        "Û∞Åª",
        "Û∞Åº",
        "Û∞ÅΩ",
        "Û∞Åæ",
        "Û∞Åø",
        "Û∞ÇÄ",
        "Û∞ÇÅ",
        "Û∞ÇÇ",
        "Û∞Åπ"
      ]
    },
    "format-full": "<span size='6pt'>BAT_f </span>{capacity}%",
    "tooltip-format-discharging": "{timeTo}",
    "tooltip-format-charging": "{timeTo}",
    "interval": 5,
    "on-click": "omarchy-menu power",
    "states": {
      "warning": 20,
      "critical": 10
    }
  },
  "bluetooth": {
    "format": "Ôäî",
    "format-disabled": "Û∞Ç≤",
    "format-connected": "Ôäî",
    "tooltip-format": "Devices connected: {num_connections}",
    "on-click": "omarchy-launch-bluetooth"
  },
  "pulseaudio": {
    "format": "<span size='6pt'>VOL </span>{volume}%",
    "on-click": "omarchy-launch-audio",
    "on-click-right": "pamixer -t",
    "tooltip-format": "Playing at {volume}%",
    "scroll-step": 5,
    "format-muted": "Óª®",
    "format-icons": {
      "headphone": "ÔÄ•",
      "headset": "ÔÄ•",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
    }
  },
  "backlight": {
        "device": "intel_backlight",
        "rotate": 0,
        "format": "{icon}",
        "tooltip-format": "üåï {percent}%",
            "format-icons": [
      "Û∞Éú",
      "Û∞Éù",
      "Û∞Éû",
      "Û∞Éü",
      "Û∞É†"
    ]
   },
   "group/tray-expander": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 600,
      "children-class": "tray-group-item"
    },
    "modules": [
      "custom/expand-icon",
      "tray",
      "backlight",
      "bluetooth"
        ]
  },
    "custom/expand-icon": {
    "format": "ÔÅì",
    "tooltip": false
  },
  "tray": {
    "icon-size": 11,
    "spacing": 5
  },
   "mpris": {
    "format": "Ó∞õ {artist} - {title}",
    "format-paused": "<span color='grey'>{status_icon}  {title}</span>",
    "max-length": 75,
    "player-icons": {
      "default": "‚è∏",
      "mpv": "üéµ"
    },
    "status-icons": {
      "paused": "Û∞ùõ"
    },
    "ignored-players": ["firefox", "chromium", "brave"]
  },
  "custom/screenrecording-indicator": {
    "on-click": "omarchy-cmd-screenrecord",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/screen-recording.sh",
    "signal": 8,
    "return-type": "json"
  },
}
