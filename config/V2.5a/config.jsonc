{
  "reload_style_on_change": true,
  "exclusive": true,
  "position": "top",
  "spacing": 0,
  "height": 0,
  "width": 1440,
  "margin-top": 0,
  "margin-bottom": 0,
  "margin-right": 0,
  "maring-left": 0,
  "modules-left": [
    "group/left1",
    "group/left2",
    "group/left3",
    "group/left4"
  ],

    "group/left1": {
    "orientation": "inherit",
    "modules": [
    "custom/omarchy"
    ]
  },

    "group/left2": {
    "orientation": "inherit",
    "modules": [
    "clock",
    "custom/idle-indicator",
    "custom/notification-silencing-indicator",
    "custom/voxtype",
    "custom/screenrecording-indicator",
    "custom/update",
    "custom/updatespacman"
    ]
  },

  "group/left3": {
    "orientation": "inherit",
    "modules": [
     "idle_inhibitor",
    "backlight",
    "bluetooth",
    "power-profiles-daemon",
    "network"
    ]
  },

    "group/left4": {
    "orientation": "inherit",
    "modules": ["mpris"]
  },

  "modules-center": ["hyprland/workspaces"],

  "modules-right": ["group/right1", "group/right"],

   "group/right1": {
    "orientation": "inherit",
    "modules": ["group/tray-expander"]
  },

   "group/right": {
    "orientation": "inherit",
    "modules": [
    "pulseaudio",
    "memory",
    "cpu",
    "battery"
  ]
  },

  "hyprland/workspaces": {
	 "on-click": "activate",
    "cursor": true,
    "format": "{icon}",
    "format-icons": {
		"1": "‰∏Ä",
		"2": "‰∫å",
		"3": "‰∏â",
		"4": "Âõõ",
		"5": "‰∫î",
		"6": "ÂÖ≠",
		"7": "‰∏É",
		"8": "ÂÖ´",
		"9": "‰πù",
		"10": "ÂçÅ"
	}
},
  "custom/omarchy": {
    "format": "Û∞Äò",
    "on-click": "omarchy-menu",
    "on-click-right": "xdg-terminal-exec",
    "tooltip-format": "Omarchy Menu\n\nSuper + Alt + Space"
  },
  "custom/update": {
    "format": "ÔÄ°",
    "exec": "omarchy-update-available",
    "on-click": "omarchy-launch-floating-terminal-with-presentation omarchy-update",
    "tooltip-format": "Omarchy update available",
    "signal": 7,
    "interval": 21600
  },
  "custom/updatespacman": {
    "format": "{}{icon}",
    "return-type": "json",
    "format-icons": {
      "has-updates": " Û∞èñ",
      "updated": ""
    },
    "exec-if": "which waybar-module-pacman-updates",
    "exec": "waybar-module-pacman-updates --no-zero-output"
  },
    "custom/voxtype": {
    "exec": "omarchy-voxtype-status",
    "return-type": "json",
    "format": "{icon}",
    "format-icons": {
      "idle": "",
      "recording": "Û∞ç¨",
      "transcribing": "Û∞îü"
    },
    "tooltip": true,
    "on-click-right": "omarchy-voxtype-config",
    "on-click": "omarchy-voxtype-model"
  },
    "custom/idle-indicator": {
    "on-click": "omarchy-toggle-idle",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/idle.sh",
    "signal": 9,
    "return-type": "json"
  },
  "custom/notification-silencing-indicator": {
    "on-click": "omarchy-toggle-notification-silencing",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/notification-silencing.sh",
    "signal": 10,
    "return-type": "json"
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "Ó∞ï",
      "deactivated": "Û∞æ´"
    }
  },
  "cpu": {
    "interval": 10,
    "format": "cpu {usage}%",
    "on-click": "omarchy-launch-or-focus-tui btop"
  },
  "memory": {
    "interval": 2,
    "format": "mem {used:0.1f}G",
    "on-click": "omarchy-launch-or-focus-tui btop"
  },
  "clock": {
    "format": "{:%H:%M}",
    "format-alt": "{:L%d %B W%V %Y}",
    "tooltip-format": "<span>{calendar}</span>",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "on-click-right": "mode",
      "format": {
        "month": "<span color='#ffead3'><b>{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b>{}</b></span>",
      },
    },
    "on-click-right": "omarchy-launch-floating-terminal-with-presentation omarchy-tz-select"
  },
 "power-profiles-daemon": {
    "format": "{icon}",
    "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
    "tooltip": true,
    "format-icons": {
      "performance": "ÓΩ∂",
      "balanced": "Ôâé",
      "power-saver": "ÔÅ¨"
    }
  },
  "network": {
    "format-icons": [
      "Û∞§Ø",
      "Û∞§ü",
      "Û∞§¢",
      "Û∞§•",
      "Û∞§®"
    ],
    "format": "{icon}",
    "format-wifi": "{icon}",
    "format-ethernet": "Û∞àÄ",
    "format-disconnected": "Û∞§Æ",
    "tooltip-format-wifi": "{essid} ({frequency} GHz)\n‚á£{bandwidthDownBytes}  ‚á°{bandwidthUpBytes}",
    "tooltip-format-ethernet": "‚á£{bandwidthDownBytes}  ‚á°{bandwidthUpBytes}",
    "tooltip-format-disconnected": "Disconnected",
    "interval": 3,
    "spacing": 1,
    "on-click": "omarchy-launch-wifi"
  },
   "battery": {
    "format": "bat {capacity}%",
    "format-discharging": "bat {capacity}%",
    "format-charging": "bat {capacity}%",
    "format-plugged": "bat_p {capacity}%",
    "format-icons": {
      "charging": ["Û∞¢ú", "Û∞ÇÜ", "Û∞Çá", "Û∞Çà", "Û∞¢ù", "Û∞Çâ", "Û∞¢û", "Û∞Çä", "Û∞Çã", "Û∞ÇÖ"],
      "default": ["Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"]
    },
    "format-full": "bat_f {capacity}%",
    "tooltip-format-discharging": "{power:>1.0f}W‚Üì {capacity}%\n {timeTo}",
    "tooltip-format-charging": "{power:>1.0f}W‚Üë {capacity}%\n {timeTo}",
    "interval": 5,
    "on-click": "omarchy-menu power",
    "states": {
      "warning": 20,
      "critical": 10
    }
  },
  "bluetooth": {
    "format": "Ôäî",
    "format-disabled": "Û∞Ç≤",
    "format-connected": "Ôäî",
    "tooltip-format": "Devices connected: {num_connections}",
    "on-click": "omarchy-launch-bluetooth"
  },
    "pulseaudio": {
    "format": "vol {volume}%",
    "on-click": "omarchy-launch-audio",
    "on-click-right": "pamixer -t",
    "tooltip-format": "Playing at {volume}%",
    "scroll-step": 5,
    "format-muted": "Óª®",
    "format-icons": {
      "headphone": "ÔÄ•",
      "headset": "ÔÄ•",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
    }
  },
  "backlight": {
        "device": "intel_backlight",
        "rotate": 0,
        "format": "{icon}",
        "tooltip-format": "üåï {percent}%",
            "format-icons": [
      "Û∞Éú",
      "Û∞Éù",
      "Û∞Éû",
      "Û∞Éü",
      "Û∞É†"
    ]
   },
  "group/tray-expander": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 600,
      "children-class": "tray-group-item"
    },
    "modules": [
      "custom/expand-icon",
      "tray"    ]
  },
    "custom/expand-icon": {
    "format": "ÔÅì",
    "tooltip": false
  },
  "tray": {
    "icon-size": 11,
    "spacing": 5
  },
   "mpris": {
    "format": "Ó∞õ  {artist} - {title}",
    "format-paused": "<span color='grey'>{status_icon}  {title}</span>",
