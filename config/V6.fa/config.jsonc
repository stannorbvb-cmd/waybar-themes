  "modules-center": [
    "hyprland/workspaces"
  ],
  "modules-right": ["group/right2", "group/right1"],

  "group/right1": {
    "orientation": "inherit",
    "modules": [
    "backlight", 
    "network",
    "bluetooth",
    "pulseaudio#output",
    "pulseaudio#input",
    "power-profiles-daemon",
    "memory",
    "cpu",
    "battery"
  ]
  },
    "group/right2": {
    "orientation": "inherit",
    "modules": ["mpris"]
