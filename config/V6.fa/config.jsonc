  "group/left1": {
    "orientation": "inherit",
    "modules": [
    "custom/omarchy",
    "clock",
    "custom/voxtype",
    "idle_inhibitor",
    "custom/screenrecording-indicator",
    "custom/updatespacman",
    "custom/update"
  ]
